class Foo {
  var a = 1;
  var increment;
  fn init(a) {
    self.a = a;
    self.increment = 2;
  };
  fn inc() {
    self.a = self.a + self.increment;
  };
  fn get() {
    var ins = self;
    self.inc();
    return ins.a;
  };
  class var c = 0;
  class fn cinc() {
    var cls = class;
    cls.c = cls.c + 1;
  };
  class fn cget() {
     return class.c;
  };
};
class Goo(Foo) {
  fn get() {
    return super.get();
  };
  class fn cget() {
     return super.cget();
  };
};
Goo.cinc();
var goo = Goo(2);
Goo.cinc();
goo.get() + Goo.cget();
